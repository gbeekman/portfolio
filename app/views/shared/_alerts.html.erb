<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Portfolio page", sticky: false ) %>
<% elsif flash[:alert] %>
  <%= js add_gritter(flash[:error], title: "Portfolio page", sticky: false ) %>
<% else %>
  <%= js add_gritter(flash[:notice], title: "Portfolio page", sticky: false ) %>
<% end %>
